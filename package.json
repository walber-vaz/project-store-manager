{
  "name": "project-store-manager",
  "version": "1.0.0",
  "description": "Simple CRUD to sales and products",
  "main": "src/server.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "test": "jest --config ./jest.config.js --runInBand",
    "test:mocha": "nyc --all --include src/models --include src/services --include src/controllers mocha tests/unit/**/*.js --exit",
    "start": "node src/server.js",
    "dev": "nodemon --ignore coverage --inspect=0.0.0.0:9229 src/server.js",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json . --ext .js, .jsx && prettier --check .",
    "lint:fix": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json . --ext .js, .jsx --fix && prettier --write .",
    "migration": "node -e \"require('./__tests__/_utils').runMigration()\"",
    "seed": "node -e \"require('./__tests__/_utils').runSeed()\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/walber-vaz/project-store-manager.git"
  },
  "author": "Walber Vaz <wvs.walber@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/walber-vaz/project-store-manager/issues"
  },
  "homepage": "https://github.com/walber-vaz/project-store-manager#readme",
  "dependencies": {
    "@hapi/boom": "9.1.0",
    "body-parser": "1.20.2",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "express-async-errors": "3.1.1",
    "helmet": "^6.1.5",
    "joi": "17.6.0",
    "morgan": "^1.10.0",
    "mysql2": "2.3.0",
    "shelljs": "0.8.5",
    "swagger-ui-express": "^4.6.2"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "chai-http": "4.3.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "frisby": "2.1.2",
    "jest": "26.4.1",
    "mocha": "10.2.0",
    "nodemon": "2.0.22",
    "nyc": "15.1.0",
    "prettier": "^2.8.8",
    "sinon": "11.1.1",
    "sinon-chai": "3.7.0"
  }
}
