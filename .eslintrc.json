{
  "root": true,
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "max-len": [
      "error",
      {
        "code": 80
      }
    ],
    "no-console": "off",
    "no-unused-vars": "off",
    "consistent-return": "off"
  }
}
